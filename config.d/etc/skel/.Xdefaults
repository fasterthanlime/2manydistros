Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintfull

URxvt.title: Terminal
URxvt.font: xft:DejaVu Sans Mono-10
URxvt.scrollBar: false
!URxvt.urgentOnBell: True

URxvt.perl-ext-common: default,selection,url-select
URxvt.keysym.M-u:      perl:url-select:select_next
URxvt.underlineURLs: true
URxvt.urlLauncher: chromium


URxvt.cursorBlink: false


! terminal colors
! ------------------------------------------------------------

URxvt*background: #1b1d1e
URxvt*foreground: #CCCCC6

!black
!*color0: #1B1D1E
!*color8: #505354
*color0:  #262729
*color8:  #554444
!red
*color1:  #F92672
*color9:  #FF669D
!green
*color2:  #A6E22E
*color10: #BEED5F
!yellow
*color3:  #FD971F
*color11: #E6DB74
!blue
*color4:  #66D9EF
*color12: #66D9EF
!magenta
*color5:  #9E6FFE
*color13: #9E6FFE
!cyan
*color6:  #5E7175
*color14: #A3BABF
!white
*color7:  #CCCCC6
*color15: #F8F8F2

